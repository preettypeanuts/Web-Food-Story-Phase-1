<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <%- include('../property/header') %>
  <style>

  </style>
  <body>

    <div style="margin: 80px 80px 80px 60px;">
      <h1 class="display-3 align-content-center"> What do you want to eat today?</h1>

    </div>

    <div>
      <div style="margin-left: 5px; margin-top: 10vh;">

  
        <div style="display: flex; flex-wrap: wrap;">
          <% postValue.forEach(el => { %>
              <div style="width: 40vh; margin-left: 15vh; margin-bottom: 10px; margin-right: 0px;">
                  <div class="card card-img-top" style="width: 25rem; min-height: 62vh;" style="width: 100%;">
                      <img src="<%= el.image %>" class="card-img-top" alt="employee pictures">
                          <div class="card-body">
                              <h5 class="card-title"><%= el.title %> </h5>
                              <p class="card-text"><%= el.description %> </p>
                              <a href="" class="btn btn-dark" style="display: flex;">
                                <p class="card-text" style="margin-right: 10px;"><%= el.likes %> </p>
                                Likes
                              </a>
                              <a href="" class="btn btn-dark" style="display: flex; margin-top: 10px;">
                                Posted By:
                                <p class="card-text" style="margin-left: 10px;"><%= el.likes %> </p>
                              </a>
                              <div>
                          </div>
                      </div>
                  </div>
              </div>
          <% }); %>
      </div>
    </div>


    <%- include('../property/footer') %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>