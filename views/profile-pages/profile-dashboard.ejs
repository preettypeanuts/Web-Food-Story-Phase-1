<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <%- include('../property/header') %>
  <style>

  </style>
  <body>

    <div style="margin-top: 7vh;">
      <div style="margin: 50px 50px 50px 50px;">

        <div style="display: flex;">
          <h1 class="display-5">Profile List</h1>
          <a href="/addUser" class="btn btn-outline-dark" style="width: max-content; height: fit-content; margin-top: 11px; margin-left: 15px;">Add Profile</a>
        </div>
        <table class="table table">
            <thead class="table-dark">
                <tr>
                    <th>Name</th>
                    <th>Bio</th>
                    <th>Action</th>

                </tr>
            </thead>
            <% profileValue.forEach(el => { %>
             
              <tbody>
                <tr>
                  <td><%= el.titleName %></td>
                  <td><%= el.bio %></td>
                  <td>
                    <a href="/editUser/<%= el.id %>" class="w-25 btn btn-warning">Edit</a>
                    <a href="/deleteUser/<%= el.id %>" class="w-25 btn btn-danger">Delete</a>
                  </td>
                </tr>
              </tbody>
              
              <% }) %>
        </table>

      </div>
    </div>


    <%- include('../property/footer') %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>